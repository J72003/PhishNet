{
    "manifest_version": 3,
    "name": "PhishNet",
    "description": "Find phishing attempts and warn the client",
    "version": "1.0",
    "action": {
        "default_popup": "index/index.html"
    },
    "icons": {
        "128": "images/lock.png",
        "256": "images/unlock.png"
    },
    "background":{
        "service_worker": "background.js",
        "type": "module"
    },
    "permissions": [
        "storage"
    ]
   
}